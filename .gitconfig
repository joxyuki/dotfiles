[user]
	email = jo.hiroyuki@lab.ntt.co.jp
	name = Hiroyuki JO
[core]
	editor = nvim
[merge]
	ff = false
[pull]
	ff = only
[alias]
	graph = log --graph --date-order --all --pretty=format:'%h %Cred%d %Cgreen%ad %Cblue%cn %Creset%s' --date=short
	co = checkout
	st = status
	d = diff
	d1 = diff HEAD~
	d2 = diff HEAD~2
[gitreview]
	username = joxyuki
[url "https://"]
	insteadOf = git://
[push]
	default = simple
